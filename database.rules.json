{
  "rules": {
    ".read": true,
    ".write": true,
    "locations": {
      "$location": {
        ".validate": "newData.hasChildren(['latitude', 'longitude', 'timestamp', 'userAgent', 'ip', 'accuracy', 'locationSource'])"
      }
    },
    "test-write": {
      ".validate": "newData.hasChild('timestamp')"
    }
  }
}
